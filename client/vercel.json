{
  "version": 2,
  "buildCommand": "npm ci --prefer-offline && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund",
  "framework": "vite",
  "functions": {},
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=4096"
    }
  }
}